/* compontents/likePage/radio/index.wxss */
@import "/colorui/icon.wxss";
@import "/colorui/main.wxss";
.radio-box {
  display: flex;
  align-items: center;
  margin-bottom: 10px;
}
.radio-box .item {
  width: 100%;
  height: 50px;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item-left {
  display: flex;
  align-items: center
}
.item-right {
  font-size: 15px;
  margin-right: 15px;
}
.radio-box .item image{
  width: 24px;
  height: 24px;
}